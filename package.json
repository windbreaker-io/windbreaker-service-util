{
  "name": "windbreaker-service-util",
  "version": "0.0.1",
  "description": "Windbreaker service utilities",
  "scripts": {
    "lint": "eslint .",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "pretest": "npm run lint",
    "test": "npm run docker:start && sleep 10 && nyc ava test/unit test/integration; npm run docker:stop",
    "test:ci": "nyc ava test/unit test/integration",
    "test:unit": "nyc ava test/unit",
    "test:integration": "npm run docker:start && sleep 10 && nyc ava test/integration; npm run docker:stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windbreaker-io/windbreaker-service-util.git"
  },
  "contributors": [
    {
      "name": "Austin Kelleher",
      "email": "a@alk.im",
      "url": "https://github.com/austinkelleher"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/windbreaker-io/windbreaker-service-util/issues"
  },
  "homepage": "https://github.com/windbreaker-io/windbreaker-service-util#readme",
  "config": {
    "branchNameCommitModifier": {
      "projectName": "WB"
    }
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "bunyan": "^1.8.10",
    "bunyan-prettystream": "^0.1.3",
    "cluster-master": "^0.2.1",
    "conflogger": "^1.0.4",
    "fashion-model": "^5.0.19",
    "fashion-model-defaults": "^1.0.1",
    "js-yaml": "^3.8.4",
    "msgpack-lite": "^0.1.26",
    "tri": "^1.0.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "branch-name-commit-modifier": "^2.0.1",
    "eslint": "^4.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "git-hooks": "^1.1.8",
    "nyc": "^11.0.2",
    "proxyquire": "^1.8.0",
    "require-self-ref": "^2.0.1",
    "sinon": "^2.3.5"
  }
}
